      agent any
      stages{
            stage("build"){
                  steps{
                        echo 'building this app'
                  }
            }

            stage("test"){
                  steps{
                        echo 'testing this app'
                  }
            }

            stage("deploy"){
                  steps{
                        echo 'deploying this app'
                  }
            }
      }
}
